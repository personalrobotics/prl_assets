<mujoco model="herring_tin">
  <compiler angle="radian"/>

  <asset>
    <texture name="herring_tin_tex" type="2d" file="herring_tin_texture.png"/>
    <material name="herring_tin_mat" texture="herring_tin_tex" specular="0.5" shininess="0.5"/>
    <mesh name="herring_tin_visual" file="herring_tin_visual.obj"/>
  </asset>

  <worldbody>
    <body name="herring_tin" pos="0 0 0.016">
      <freejoint name="herring_tin_joint"/>

      <!-- Collision geometry - flat rectangular tin -->
      <geom name="herring_tin_collision"
            type="box"
            size="0.075 0.016 0.042"
            mass="0.2"
            friction="0.6 0.005 0.0001"
            contype="1"
            conaffinity="1"
            rgba="0 0 0 0"/>

      <!-- Visual mesh -->
      <geom name="herring_tin_visual"
            type="mesh"
            mesh="herring_tin_visual"
            material="herring_tin_mat"
            contype="0"
            conaffinity="0"/>
    </body>
  </worldbody>
</mujoco>
