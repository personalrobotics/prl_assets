<mujoco model="lunchbox">
  <compiler angle="radian"/>

  <asset>
    <texture name="lunchbox_tex" type="2d" file="lunchbox_texture.png"/>
    <material name="lunchbox_mat" texture="lunchbox_tex" specular="0.5" shininess="0.5"/>
    <mesh name="lunchbox_visual" file="lunchbox_visual.obj"/>
  </asset>

  <worldbody>
    <body name="lunchbox" pos="0 0 0.049">
      <freejoint name="lunchbox_joint"/>

      <!-- Collision geometry - box -->
      <geom name="lunchbox_collision"
            type="box"
            size="0.13 0.1 0.049"
            mass="0.4"
            friction="0.7 0.005 0.0001"
            contype="1"
            conaffinity="1"
            rgba="0 0 0 0"/>

      <!-- Visual mesh -->
      <geom name="lunchbox_visual"
            type="mesh"
            mesh="lunchbox_visual"
            material="lunchbox_mat"
            contype="0"
            conaffinity="0"/>
    </body>
  </worldbody>
</mujoco>
