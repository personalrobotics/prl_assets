<mujoco model="plastic_plate">
  <compiler angle="radian"/>

  <asset>
    <material name="plastic_plate_mat" rgba="0.8 0.8 0.8 1.0" specular="0.5" shininess="0.5"/>
    <mesh name="plastic_plate_visual" file="plastic_plate_visual.obj" scale="1 1 1"/>
  </asset>

  <worldbody>
    <body name="plastic_plate" pos="0 0 0.0135">
      <freejoint name="plastic_plate_joint"/>

      <!-- Collision geometry - cylinder approximation -->
      <geom name="plastic_plate_collision"
            type="cylinder"
            size="0.133 0.0135"
            mass="0.12"
            friction="0.6 0.005 0.0001"
            contype="1"
            conaffinity="1"
            rgba="0 0 0 0"/>

      <!-- Visual mesh -->
      <geom name="plastic_plate_visual"
            type="mesh"
            mesh="plastic_plate_visual"
            material="plastic_plate_mat"
            contype="0"
            conaffinity="0"
            pos="0 0 -0.0135"/>
    </body>
  </worldbody>
</mujoco>
